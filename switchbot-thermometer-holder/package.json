{
  "name": "switchbot-thermometer-holder",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "dependencies": {
    "@jscad/csg": "^0.7.0",
    "common": "file:../common"
  },
  "devDependencies": {
    "@jscad/cli": "^1.10.0",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "yarn build:tsc && yarn build:openjscad",
    "build:tsc": "tsc",
    "build:openjscad": "yarn build:openjscad:holder && yarn build:openjscad:hook",
    "build:openjscad:holder": "openjscad dist/holder.js",
    "build:openjscad:hook": "openjscad dist/hook.js",
    "clean": "rm -rf dist",
    "fmt": "prettier --write *.json src/*.ts src/**/*.ts"
  }
}
